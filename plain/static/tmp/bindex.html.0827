  var htmlstr = tpl(context);

  console.log(htmlstr);
  return htmlstr;
}

function mk_ul($, list){
  var htags = _.map(list, function(file_meta){
      return render($, file_meta);
  });

  var ul = '<ul id="file-list-ul">';
  ul += htags.join("\n");
  ul += '</ul>';

  console.log('mk ul: ', ul);

  return ul;
}


